{
  "name": "ngx-bootstrap-feedback",
  "version": "1.0.0-alpha2",
  "description": "Feedback modal powered by bootstrap for your Angular application.",
  "main": "./bundles/ngx-bootstrap-feedback.umd.js",
  "private": true,
  "peerDependencies": {
    "@angular/core": "^2.4.0 || ^4.0.0",
    "@herbis/ngx-modal": "^0.1.0",
    "rxjs": "^5.0.1"
  },
  "dependencies": {},
  "devDependencies": {
    "@angular/common": "^4.1.3",
    "@angular/compiler": "^4.1.3",
    "@angular/compiler-cli": "^4.1.3",
    "@angular/core": "^4.1.3",
    "@angular/platform-browser": "^4.1.3",
    "@angular/router": "^4.1.3",
    "@herbis/ngx-modal": "^0.1.0",
    "codelyzer": "~2.0.0",
    "copyfiles": "^1.2.0",
    "rollup": "^0.42.0",
    "rollup-plugin-alias": "^1.3.1",
    "rollup-plugin-angular": "^0.4.4",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rxjs": "^5.0.1",
    "tslint": "4.5.0",
    "typescript": "^2.3.4",
    "uglify-js": "^3.0.15",
    "zone.js": "^0.8.11"
  },
  "scripts": {
    "clean": "rm -rf build",
    "transpile": "ngc",
    "package": "rollup -c",
    "minify": "uglifyjs build/dist/bundles/ngx-bootstrap-feedback.umd.js --screw-ie8 --compress --mangle --comments --output build/dist/bundles/ngx-bootstrap-feedback.umd.min.js",
    "prebuild": "npm run clean",
    "build": "npm run lint && npm run transpile && npm run package && npm run minify",
    "postbuild": "copyfiles -f src/**/*.html package.json README.md build/dist",
    "lint": "tslint src/**/*.ts",
    "prepublishBeta": "npm run build",
    "publishBeta": "npm publish build/dist --tag beta"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Herbis/ngx-bootstrap-feedback.git"
  },
  "keywords": [
    "feedback",
    "bootstrap",
    "modal",
    "angular"
  ],
  "author": "Herberts Markuns",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Herbis/ngx-bootstrap-feedback/issues"
  },
  "homepage": "https://github.com/Herbis/ngx-bootstrap-feedback#readme",
  "types": "feedback.module.d.ts"
}
